{
  "name": "wreq-js",
  "version": "1.3.0",
  "description": "High-performance HTTP client for Node.JS with real-browser TLS and HTTP/2 fingerprints, powered by Rust",
  "main": "dist/wreq-js.js",
  "types": "dist/wreq-js.d.ts",
  "exports": {
    ".": {
      "types": "./dist/wreq-js.d.ts",
      "require": "./dist/wreq-js.js",
      "import": "./dist/wreq-js.js"
    }
  },
  "scripts": {
    "build": "npm run build:rust && npm run build:ts",
    "build:rust": "napi build --platform --release --manifest-path rust/Cargo.toml",
    "build:ts": "npm run clean:dist && tsc",
    "clean:dist": "rimraf dist",
    "artifacts": "napi artifacts",
    "clean": "rimraf dist rust/target rust/*.node",
    "test": "npm run build && node dist/test/run-with-local-server.js",
    "test:coverage": "npm run build && c8 --reporter=html --reporter=text --reporter=lcov node dist/test/run-with-local-server.js",
    "test:coverage:ci": "npm run build && c8 --reporter=text-summary --reporter=lcov node dist/test/run-with-local-server.js",
    "test:coverage:report": "c8 report --reporter=text",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "ci:biome": "biome ci ."
  },
  "keywords": [
    "browser-emulation",
    "fingerprint",
    "tls",
    "http2",
    "impersonation",
    "web-scraping",
    "web-scraper",
    "crawler",
    "fetch",
    "http-client",
    "rust",
    "neon",
    "native-addon",
    "ja3",
    "ja4",
    "tls-fingerprint",
    "wreq",
    "browser"
  ],
  "author": "Oleksandr Herasymov <herasymov7@icloud.com>",
  "contributors": [
    "will-work-for-meal"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sqdshguy/wreq-js.git"
  },
  "bugs": {
    "url": "https://github.com/sqdshguy/wreq-js/issues"
  },
  "homepage": "https://github.com/sqdshguy/wreq-js#readme",
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@napi-rs/cli": "^3.5.0",
    "@types/node": "^24.10.1",
    "c8": "^10.1.3",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "files": [
    "dist",
    "rust/*.node"
  ],
  "napi": {
    "binaryName": "wreq-js",
    "targets": [
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "aarch64-unknown-linux-gnu",
      "x86_64-unknown-linux-musl",
      "x86_64-pc-windows-msvc"
    ]
  }
}
